{
  "name": "zuudeui-packages",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish",
    "ios-mockup:build": "cd packages/ios-mockups && pnpm build",
    "cropper:build": "cd packages/cropper && pnpm build",
    "g": "tsx script.ts && prettier --write ./apps/ios-mockups/__registry__/index.ts",
    "p:script": "tsx package-script.ts"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "prettier": "^3.5.1",
    "turbo": "^2.4.2",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.5.3",
    "tsx": "^4.19.4"
  }
}
